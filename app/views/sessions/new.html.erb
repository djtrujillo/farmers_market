<%= form_for(:session, url: login_path) do |f| %>

<%= f.label :email %>
<%= f.text_field :email %>

<%= f.label :password %>
<%= f.text_field :password %>

<%= f.submit "Login" %>

<% end %>

<%= link_to "Create Account", new_user_path  %>
<%= link_to "Sign in with Google", "/auth/google_oauth2", id: "sign_in" %>
